{
  "name": "rapid-mono",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:js": "esbuild static/js/index.js --bundle --outfile=./dist/index.js",
    "watch:js": "esbuild static/js/index.js --bundle --outfile=./dist/index.js --watch",
    "build:css": "tailwindcss -i static/css/*.css -o dist/index.css",
    "watch:css": "tailwindcss -i static/css/*.css -o dist/index.css -w",
    "dev": "npm-run-all --parallel watch:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@lottiefiles/dotlottie-wc": "^0.2.23",
    "animate.css": "^4.1.1",
    "htmx-ext-preload": "^2.0.1",
    "htmx.org": "^1.9.12",
    "lucide": "^0.454.0"
  },
  "devDependencies": {
    "esbuild": "0.24.0",
    "fluid-tailwind": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.14"
  }
}
